file = {:home/aijjeh/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Azimi, Eslamlou, Pekcan - 2020 - Data-driven structural health monitoring and damage detection through deep learning State-ofthe- art re.pdf:pdf},
isbn = {1311416846},
