doi = {10.1063/1.5031651},
file = {:home/aijjeh/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Melville et al. - 2018 - Structural damage detection using deep learning of ultrasonic guided waves.pdf:pdf},
