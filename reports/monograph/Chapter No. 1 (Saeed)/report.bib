file = {:C$\backslash$:/Users/Saeed Ullah/Desktop/SHM related papers/literature review/Just for Citations/Staszewski{\_}2007{\_}Meas.{\_}Sci.{\_}Technol.{\_}18{\_}024.pdf:pdf},
issn = {13616501},
journal = {Measurement Science and Technology},
keywords = {3D laser vibrometer,Crack detection,Fatigue crack,Lamb waves,Non-destructive testing},
number = {3},
pages = {727--739},
title = {{Fatigue crack detection in metallic structures with Lamb waves and 3D laser vibrometry}},
volume = {18},
