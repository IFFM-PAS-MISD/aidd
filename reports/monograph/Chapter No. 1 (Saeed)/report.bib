doi = {10.1016/j.compscitech.2003.10.011},
file = {:C$\backslash$:/Users/Saeed Ullah/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Toyama, Takatsubo - 2004 - Lamb wave method for quick inspection of impact-induced delamination in composite laminates.pdf:pdf},
