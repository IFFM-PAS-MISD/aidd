file = {:C$\backslash$:/Users/Saeed Ullah/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kudela, Radzienski, Ostachowicz - 2018 - Impact induced damage assessment by means of Lamb wave image processing.pdf:pdf},
issn = {10961216},
journal = {Mechanical Systems and Signal Processing},
keywords = {Delamination,Impact damage,Lamb waves,Laser vibrometry,Wavefield,Wavenumber filtering},
pages = {23--36},
publisher = {Elsevier Ltd},
title = {{Impact induced damage assessment by means of Lamb wave image processing}},
url = {https://doi.org/10.1016/j.ymssp.2017.09.020},
volume = {102},
year = {2018}
}
