issn = {10961216},
journal = {Mechanical Systems and Signal Processing},
keywords = {Damage localization,Lamb waves,PZT,SHM,Sensor array,Wave focusing},
pages = {21--32},
publisher = {The Authors},
title = {{Structural Health Monitoring system based on a concept of Lamb wave focusing by the piezoelectric array}},
url = {https://doi.org/10.1016/j.ymssp.2018.02.008},
volume = {108},
year = {2018}
}
