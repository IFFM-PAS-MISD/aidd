journal = {Journal of Composites Science},
keywords = {condition-based maintenance,damage detection,pod,probability of detection},
number = {1},
pages = {13},
title = {{Structural Health Monitoring for Advanced Composite Structures: A Review}},
volume = {4},
year = {2020}
}
@article{Crane2017,
abstract = {Monitoring a structure to track both the material and component degradation has long been a goal of the aircraft maintenance community. Research with this goal has concentrated on investigating new and novel instrumentation and analysis capabilities to make structural health monitoring (SHM) a reality in order to reduce unnecessary inspections and unscheduled maintenance actions. While research efforts have shown promise for metal components, those involving composite materials have not achieved the same level of capability or acceptance. Without a SHM capability the composite community has relied on detailed inspections during periodic maintenance actions to detect both structural damage and gage the extent of material degradation. Types of damage and degraded material properties include impact damage with the associated subsurface delaminations, debonding of fibers from the matrix at stress concentrations, corrosion of the fasteners used to join composites to metals, and failing adhesive bonds. This approach is expensive and reduces the economic advantage that composites offer the designe.},
