pages = {456--466},
title = {{Identification of cracks in thin-walled structures by means of wavenumber filtering}},
url = {http://gateway.webofknowledge.com/gateway/Gateway.cgi?GWVersion=2&SrcAuth=ORCID&SrcApp=OrcidOrg&DestLinkType=FullRecord&DestApp=WOS_CPL&KeyUT=WOS:000343687200030&KeyUID=WOS:000343687200030 https://linkinghub.elsevier.com/retrieve/pii/S0888327014002179},
