%%footnote
\def\dblcolfootnote{\@ifnextchar[\@xdblcolfootnote{\stepcounter\@mpfn
     \protected@xdef\@thefnmark{\thempfn}%
     \@footnotemark\@dblcolfootnotetext}}
     
\def\@xdblcolfootnote[#1]{%
   \begingroup
     \csname c@\@mpfn\endcsname #1\relax
     \unrestored@protected@xdef\@thefnmark{\thempfn}%
   \endgroup
   \@footnotemark\@dblcolfootnotetext}

\def\dblcolfootnotetext{%
     \@ifnextchar [\@xdblcolfootnotenext
       {\protected@xdef\@thefnmark{\thempfn}%
    \@dblcolfootnotetext}}
\def\@xdblcolfootnotenext[#1]{%
  \begingroup
     \csname c@\@mpfn\endcsname #1\relax
     \unrestored@protected@xdef\@thefnmark{\thempfn}%
  \endgroup
  \@dblcolfootnotetext}

\newinsert\dblcolfootins
\skip\dblcolfootins=\skip\footins % space added when footnote is present
\count\dblcolfootins=1000 % footnote magnification factor (1 to 1)
\dimen\dblcolfootins=8in % maximum footnotes per page

\newcommand\@dblcolfootnotetext[1]{%
      \insert\dblcolfootins{%
        \ifFN@setspace
          \let\baselinestretch\FN@baselinestretch
        \fi
        \reset@font\footnotesize
        \interlinepenalty\interfootnotelinepenalty
        \splittopskip\footnotesep
        \splitmaxdepth \dp\strutbox
        \floatingpenalty\@MM
        \hsize\textwidth
        \@parboxrestore
        \protected@edef\@currentlabel{%
          \csname p@footnote\endcsname\@thefnmark
        }%
        \color@begingroup
          \@makefntext{%
            \rule\z@\footnotesep
            \ignorespaces#1\@finalstrut\strutbox
          }%
        \color@endgroup
      }%
      \FN@mf@prepare
    }%
    
\newif\if@elsdblcolfootinsdone\@elsdblcolfootinsdonefalse

\patchcmd{\@outputdblcol}{\@combinedblfloats}
    {\@combinedblfloats%
       \ifvoid\dblcolfootins%
       \else%
         \@tempdima=\dimexpr(\ht\dblcolfootins + %
                     \skip\dblcolfootins)%
         \immediate\write\@auxout{%
            \string\gdef\string\dblcolfnht{\the\@tempdima}}%
         \setbox\@outputbox \vbox {%
           \boxmaxdepth \@maxdepth
           \unvbox \@outputbox
          \vskip \skip\dblcolfootins
           \color@begingroup
             \normalcolor
             \footnoterule
             \unvbox \dblcolfootins
           \color@endgroup
           }%
       \fi
    }
    {}{}

