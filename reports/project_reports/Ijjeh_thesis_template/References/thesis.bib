title = {{Application of elastic waves and neural networks for the prediction of forces in bolts of flange connections subjected to static tension tests}},
url = {https://www.mdpi.com/1996-1944/13/16/3607/htm https://www.mdpi.com/1996-1944/13/16/3607},
