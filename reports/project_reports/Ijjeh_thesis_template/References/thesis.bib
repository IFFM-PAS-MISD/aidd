keywords = {Cantilever beam,Damage severity quantification,Damage type classification,NMBF,Output noise,PCA,SBF,SVM,Simulation},
pages = {136--146},
title = {{Automatic damage type classification and severity quantification using signal based and nonlinear model based damage sensitive features}},
url = {https://reader.elsevier.com/reader/sd/pii/S0959152418301975?token=01B16188891FCDF7C1F1F7829A88893EC18BE23510EF84BE175824FB8B239D6C84AC0CB73A309A65FF69FF9394AF41E5&originRegion=eu-west-1&originCreation=20210929101153},
