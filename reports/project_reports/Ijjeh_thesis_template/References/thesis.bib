issn = {2504477X},
keywords = {Condition-based maintenance,Damage detection algorithm,Probability of detection (POD),Sensors},
number = {1},
pages = {13},
title = {{Structural health monitoring for advanced composite structures: A review}},
volume = {4},
year = {2020}
}
