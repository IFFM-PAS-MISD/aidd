isbn = {9783030649074},
issn = {23662565},
keywords = {Deep learning,Inverse problems,Material property identification,Ultrasonic guided waves},
pages = {77--90},
title = {{Deep Learning Based Identification of Elastic Properties Using Ultrasonic Guided Waves}},
