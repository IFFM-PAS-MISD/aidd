doi = {10.3390/s19061453},
issn = {1424-8220},
journal = {Sensors (Basel, Switzerland)},
keywords = {discrimination model,kudzu powder,laser-induced breakdown spectroscopy,rapid identification},
month = {mar},
number = {6},
pages = {1453},
pmid = {30934580},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Rapid Identification of Kudzu Powder of Different Origins Using Laser-Induced Breakdown Spectroscopy.}},
