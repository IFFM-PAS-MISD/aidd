}

@InProceedings{Rovere1997,
  Title                    = {Catamaran Resistance from Tests on a Single Demihull},
  Author                   = {Rovere, J. E.},
  Booktitle                = {Proceedings 4th International Conference of Fast Sea Transportation (FAST’97)},
  Year                     = {1997},

  Address                  = {Sydney, Australia},
  Pages                    = {737-741},
  Publisher                = {ADI Limited, Australia},
  Volume                   = {2},

  Abstract                 = {The prediction of the wave-making resistance associated with the forward motion of a ship has been the subject of research for close to a century Early studies by Michell (1898), Havelock (7909) and Lunde (7957) have provided accepted theories for the calculation of the wave-making resistance. Various towing-tank techniques have also been used for the same purpose of predicting the wave-making resistance of a full size ship. This paper presents a novel technique to predict the effects of demihull spacing on the wave-making resistance of a catamaran by testing one of its demihulls in close proximity to a towing tank wall, The tank wall acts as the plane of symmetry of the catamaran, reflecting waves generated by the demihull, and thereby provides the correct wave interference and blockage effects from the non-existent hull. A series of model tests on a catamaran and an equivalent-demihull was the subject of the experimental testing. Hull spacing-to-model-length ratios of 0.2, 0.3, 0.4, and 0.5, were tested over a range of speeds in two water depths and two tank widths. A large number of experimental results and theoretical computations have been made to examine the hypothesis of testing half of the catamaran, and it is shown that the demihull approach can be used to accurately predict the influence of the catamaran hull spacing. },
  Keywords                 = {Single demihull
Resistance},
