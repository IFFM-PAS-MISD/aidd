pages = {946--958},
publisher = {SAGE Publications Ltd},
title = {{Micro-cracking monitoring and fracture evaluation for crumb rubber concrete based on acoustic emission techniques}},
url = {http://journals.sagepub.com/doi/10.1177/1475921717730538},
volume = {17},
year = {2018}
}
