keywords = {Operational modal analysis,physical interpretation,principal component analysis,three-dimensional,time-domain statistical modal parameter identifica},
month = {5},
number = {1-4},
pages = {137--144},
publisher = {IOS Press},
title = {{Principal component analysis based three-dimensional operational modal analysis}},
