title = {{Damage Localisation in a Stiffened Composite Panel}},
url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1475-1305.2007.00371.x},
volume = {44},
year = {2008}
}
@article{Ihn2008,
abstract = {This study presents active sensing methods in structural health monitoring, for detecting cracks and debonds in metallic and composite structures, which can be potentially implemented into airframe structures. First, a pitch-catch method using a pair of piezoelectric actuator and sensor is introduced to generate a damage indeX which can be used to characterize damage at a known location. Tests on airbus fuselage panels are conducted to verify the method and damage indeX. The damage indeX relates changes in the energy content of a specific Lamb wave mode selected by group velocity analysis to the eXtent of damage. Second, an imaging method based on multiple pitch-catch information, a network of piezoelectric actuator/sensors, is presented for characterizing damage (location and size) without need for a structural or damage model. The imaging method with an autofocusing feature is applied to aluminum plates and a stiffened composite panel for method verification. {\textcopyright} 2008 Sage Publications.},
author = {Ihn, Jeong Beom and Chang, Fu Kuo},
doi = {10.1177/1475921707081979},
issn = {14759217},
journal = {Structural Health Monitoring},
keywords = {Cracks,Delamination,Diagnostic imaging,Lamb waves,Piezoelectric actuators,Structural health monitoring},
month = {3},
number = {1},
pages = {5--19},
title = {{Pitch-catch active sensing methods in structural health monitoring for aircraft structures}},
volume = {7},
year = {2008}
}
@inproceedings{Schubert2008,
abstract = {The combination of an array of distributed transducers permanently coupled to the structure under investigation and the possibility to compare the actual state of the structure with a predefined reference state offers totally new perspectives for elastic wave based signal processing and imaging techniques. In the present paper a multi-level approach for the evaluation of guided elastic wave data obtained by an array of distributed sensors and actuators is presented. For this purpose a numerical simulation platform has been developed that allows extensive 3-D calculations of guided wave propagation including appropriate signal processing and data evaluation algorithms. Calculations in a quasi-isotropic plate are used to exemplary demonstrate various imaging approaches. The results reveal that imaging techniques based on distributed transducer arrays are able to increase the significance of monitoring results and should therefore lead to a considerably better performance and enhanced acceptance of guided wave based SHM systems in the near future.},
author = {Schubert, F.},
booktitle = {Proc. Structural Health Monitoring},
isbn = {9781932078947},
pages = {748--757},
title = {{A conceptual study on guided wave based imaging techniques for SHM with distributed transducer array}},
year = {2008}
}
@book{Grosse2008,
abstract = {Acoustic Emission (AE) techniques have been studied in civil engineering for a long time. The techniques are recently going to be more and more applied to practical applications and to be standardized in the codes. This is because the increase of aging structures and disastrous damages due to recent earthquakes urgently demand for maintenance and retrofit of civil structures in service for example. It results in the need for the development of advanced and effective inspection techniques. Thus, AE techniques draw a great attention to diagnostic applications and in material testing. The book covers all levels from the description of AE basics for AE beginners (level of a student) to sophisticated AE algorithms and applications to real large-scale structures as well as the observation of the cracking process in laboratory specimen to study fracture processes. {\textcopyright} 2008 Springer-Verlag Berlin Heidelberg.},
